{
  "name": "embedd",
  "version": "1.0.1",
  "description": "Embed reddit comments about your page on your page!",
  "main": "src/app.js",
  "scripts": {
    "pretest": "standard --verbose | snazzy",
    "test": "babel-node --presets es2015 tests/*.js | faucet",
    "start": "webpack-dev-server --output-filename testBundle.js --content-base tests/",
    "build": "webpack --progress --colors --optimize-minimize --output-filename embedd.min.js"
  },
  "author": "Tim Gallant",
  "license": "BSD-2-Clause",
  "dependencies": {
    "async": "2.6.1",
    "axios": "^0.18.0",
    "mustache": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-preset-es2015": "~6.24.1",
    "css-loader": "~1.0.0",
    "faucet": "~0.0.1",
    "html-loader": "~0.4.5",
    "node-bourbon": "~4.2.8",
    "node-neat": "~1.7.2",
    "node-sass": "^4.10.0",
    "sass-loader": "~6.0.6",
    "snazzy": "^8.0.0",
    "standard": "^12.0.0",
    "style-loader": "~0.20.0",
    "tape": "~4.9.0",
    "webpack": "~3.11.0",
    "webpack-dev-server": "~2.11.1"
  }
}
