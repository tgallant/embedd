{
  "name": "embedd",
  "version": "1.0.1",
  "description": "Embed reddit comments about your page on your page!",
  "main": "src/app.js",
  "scripts": {
    "pretest": "standard --verbose | snazzy",
    "test": "babel-node --presets es2015 tests/*.js | faucet",
    "start": "webpack-dev-server --output-filename testBundle.js --content-base tests/",
    "build": "webpack --progress --colors --optimize-minimize --output-filename embedd.min.js"
  },
  "author": "Tim Gallant",
  "license": "BSD-2-Clause",
  "dependencies": {
    "async": "2.4.1",
    "axios": "^0.16.0",
    "mustache": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.1",
    "babel-loader": "~7.0.0",
    "babel-preset-es2015": "~6.24.1",
    "css-loader": "~0.28.2",
    "faucet": "~0.0.1",
    "html-loader": "~0.4.5",
    "node-bourbon": "~4.2.8",
    "node-neat": "~1.7.2",
    "node-sass": "~4.5.3",
    "sass-loader": "~6.0.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "~0.18.0",
    "tape": "~4.6.0",
    "webpack": "~2.5.1",
    "webpack-dev-server": "~2.4.5"
  }
}
